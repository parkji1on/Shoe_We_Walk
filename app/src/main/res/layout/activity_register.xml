<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/SWW_Theme_Color"
    tools:context=".RegisterActivity">


    <TextView
        android:id="@+id/homeMyShoesTitleText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="회원가입"
        android:textColor="@color/white"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/RegisterLogoImage"
        android:layout_width="180dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/sww_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/homeMyShoesTitleText2" />

    <ImageView
        android:id="@+id/EditProfileWeightBox"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/login_textbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditProfileHightBox" />

    <ImageView
        android:id="@+id/EditProfileAgeBox"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/login_textbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditProfileGenderBox" />

    <ImageView
        android:id="@+id/EditProfileHightBox"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/login_textbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditProfileAgeBox" />

    <ImageView
        android:id="@+id/EditProfileNameBox"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/login_textbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RegisterLogoImage" />

    <ImageView
        android:id="@+id/EditProfileGenderBox"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/login_textbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditProfileNameBox" />

    <TextView
        android:id="@+id/EditProfileUserName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:alpha="0.7"
        android:text="이름"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileNameBox"
        app:layout_constraintStart_toStartOf="@+id/EditProfileNameBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileNameBox"
        app:layout_constraintVertical_bias="0.5" />

    <EditText
        android:id="@+id/EditProfileUserNameEditText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:alpha="0.7"
        android:background="@null"
        android:ems="3"
        android:gravity="right"
        android:hint="홍길동"
        android:inputType="textPersonName"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileNameBox"
        app:layout_constraintEnd_toEndOf="@+id/EditProfileNameBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileNameBox"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/EditProfileUserGender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:alpha="0.7"
        android:text="성별"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileGenderBox"
        app:layout_constraintStart_toStartOf="@+id/EditProfileGenderBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileGenderBox"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/EditProfileUserAge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:alpha="0.7"
        android:text="나이"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileAgeBox"
        app:layout_constraintStart_toStartOf="@+id/EditProfileAgeBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileAgeBox"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/EditProfileUserGenderText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:alpha="0.7"
        android:text="남성"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileGenderBox"
        app:layout_constraintEnd_toEndOf="@+id/EditProfileGenderBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileGenderBox"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/EditProfileUserAgeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:alpha="0.7"
        android:text="세"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileAgeBox"
        app:layout_constraintEnd_toEndOf="@+id/EditProfileAgeBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileAgeBox"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/EditProfileUserHight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:alpha="0.7"
        android:text="키"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileHightBox"
        app:layout_constraintStart_toStartOf="@+id/EditProfileHightBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileHightBox"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/EditProfileUserHightText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:alpha="0.7"
        android:text="cm"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileHightBox"
        app:layout_constraintEnd_toEndOf="@+id/EditProfileHightBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileHightBox"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/EditProfileUserWeight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:alpha="0.7"
        android:text="몸무게"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileWeightBox"
        app:layout_constraintStart_toStartOf="@+id/EditProfileWeightBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileWeightBox"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/EditProfileUserWeightText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:alpha="0.7"
        android:text="kg"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileWeightBox"
        app:layout_constraintEnd_toEndOf="@+id/EditProfileWeightBox"
        app:layout_constraintTop_toTopOf="@+id/EditProfileWeightBox"
        app:layout_constraintVertical_bias="0.5" />

    <ImageView
        android:id="@+id/EditProfileEditImage"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/regesterbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditProfileWeightBox" />

    <EditText
        android:id="@+id/EditProfileUserAgeEditText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:alpha="0.7"
        android:background="@null"
        android:ems="3"
        android:gravity="right"
        android:hint="20"
        android:inputType="numberSigned"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileAgeBox"
        app:layout_constraintEnd_toStartOf="@+id/EditProfileUserAgeText"
        app:layout_constraintTop_toTopOf="@+id/EditProfileAgeBox"
        app:layout_constraintVertical_bias="0.5" />

    <EditText
        android:id="@+id/EditProfileUserHightEditText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:alpha="0.7"
        android:background="@null"
        android:ems="3"
        android:gravity="right"
        android:hint="180"
        android:inputType="numberSigned"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileHightBox"
        app:layout_constraintEnd_toStartOf="@+id/EditProfileUserHightText"
        app:layout_constraintTop_toTopOf="@+id/EditProfileHightBox"
        app:layout_constraintVertical_bias="0.5" />

    <EditText
        android:id="@+id/EditProfileUserWeightEditText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="3dp"
        android:alpha="0.7"
        android:background="@null"
        android:ems="3"
        android:gravity="right"
        android:hint="80"
        android:inputType="numberDecimal"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/EditProfileWeightBox"
        app:layout_constraintEnd_toStartOf="@+id/EditProfileUserWeightText"
        app:layout_constraintTop_toTopOf="@+id/EditProfileWeightBox"
        app:layout_constraintVertical_bias="0.5" />

</androidx.constraintlayout.widget.ConstraintLayout>